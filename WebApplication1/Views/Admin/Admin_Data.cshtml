@model IEnumerable<WebApplication1.Models.Company_Admin>

@{
    ViewBag.Title = "List";
}
<div style="margin-top:100px">
    <h2 style=" text-align :center">Admin Data</h2>

    <p style="text-align:center">
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table-bordered" style="margin-left:600px">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Admin_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Admin_Pass)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Admin_Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Admin_Phone)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Admin_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Admin_Pass)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Admin_Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Admin_Phone)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Admin_Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Admin_Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Admin_Id })
                </td>
            </tr>
        }

    </table>
</div>
