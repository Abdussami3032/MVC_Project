                        &nbsp;
                        <p>@Html.DisplayFor(modelItem => item.Cust_Pass)</p>
                        &nbsp;
                        <br />
                        <p>@Html.DisplayNameFor(model => model.Cust_CNIC)&nbsp;&nbsp;:</p>
                        &nbsp;
                        <p>@Html.DisplayFor(modelItem => item.Cust_CNIC)</p>
                        <br />
                        <p>@Html.DisplayFor(modelItem => item.UserName)</p>
                        <br />


                        @Html.ActionLink("Edit", "Edit", new { id = item.Cust_Id }) |
                        @Html.ActionLink("Details", "Details", new { id = item.Cust_Id }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.Cust_Id })

                    </div>
