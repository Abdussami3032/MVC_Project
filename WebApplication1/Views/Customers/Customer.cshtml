            <div class="col-lg-4 col-md-6 grids-feature">
                <div class="row1">
                    <div class="area-box">

                        <h4>
                            @Html.DisplayNameFor(model => model.Cust_Name) &nbsp;&nbsp:
                            &nbsp;
